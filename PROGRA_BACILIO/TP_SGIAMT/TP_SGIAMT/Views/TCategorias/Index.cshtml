@model IEnumerable<TP_SGIAMT.Models.TCategoria>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VcNomCategoria)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.VcNomCategoria)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PkIcCodCat">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PkIcCodCat">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PkIcCodCat">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
